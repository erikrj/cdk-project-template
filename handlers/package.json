{
  "name": "handlers",
  "type": "module",
  "scripts": {
    "prebuild": "prettier --check . && eslint .",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest run --coverage",
    "clean": "find ./src -type f \\( -name \"*.mjs\" -o -name \"*.d.mts\" -o -name \"*.map\" \\) -exec rm -f {} \\;",
    "fmt": "prettier --write ."
  },
  "dependencies": {
    "@nr1e/logging": "1.4.1",
    "uuid": "^11.0.3",
    "valibot": "^0.42.1",
